package game;

import java.awt.Component;
import java.awt.Graphics;

import javax.swing.ImageIcon;

public class CarObstacle extends Component{

	private ImageIcon car;
	private int x;
	private int y;

	public CarObstacle() {
		this.car = new ImageIcon("yellowCar.jpg");
	}
	
	protected void paintComponent(Graphics g) {
		this.car.paintIcon(this, g, x, y);
	}
}
